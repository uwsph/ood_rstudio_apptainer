<%-
  cpus = sph_cpus.to_i
  mem = sph_mem.to_i
-%>
---
batch_connect:
  template: "basic"
  conn_params:
    - csrf_token
script:
  accounting_id: "<%= auto_accounts %>"
  queue_name: "<%= auto_queues %>"
  native:
    - "--cpus-per-task"
    - "<%= cpus %>"
    - "--mem"
    - "<%= mem * 1024 %>"
